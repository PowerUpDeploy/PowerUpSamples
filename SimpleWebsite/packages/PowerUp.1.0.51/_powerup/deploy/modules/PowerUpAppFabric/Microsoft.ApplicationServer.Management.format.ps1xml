    <View>
      <Name>InstanceStoreInfoView</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.ApplicationServer.Management.Data.InstanceStoreInfo</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <PropertyName>Name</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>ProviderName</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>SiteName</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>VirtualPath</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Parameters</Label>
                <ScriptBlock>
			$params = $_.Parameters
			$entries = @()
			$params.GetEnumerator() | %{ $entry = New-Object System.Collections.DictionaryEntry($_, $params[$_]); $entries += $entry }
			$entries | Format-Table -View InstanceStoreParametersView | Out-String
		</ScriptBlock>
              </ListItem>
              <ListItem>
                <PropertyName>IsLocal</PropertyName>
              </ListItem>              
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>

