<?xml version="1.0"?>
<package>
	<metadata>
		<id>PowerUp.SampleWebsite</id>
		<title>PowerUp Sample Website</title>
		<version>1.0.1</version>
		<authors>Paul Suart, Andrew Revell</authors>
		<owners>Paul Suart, Andrew Revell</owners>
		<projectUrl>https://github.com/PowerUpDeploy/PowerUpSamples</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>An example of how to get started with PowerUp build and deployment framework</description>
		<summary>Contains a simple website that's built and deployed using PowerUp</summary>
		<copyright>Copyright 2014</copyright>
		<dependencies>
			<dependency id="PowerUp" />
		</dependencies>
		<tags>psake powershell build deploy configuration ci example quickstart</tags>
	</metadata>  
	<files>
		<file src="_tools\**\*.*" target="Tools" />
		<file src="_templates\**\*.*" target="Files\_templates" />
		<file src="SimpleWebsite\**\*.*" target="Files\SimpleWebsite" />
		<file src="bootstrap.bat" target="Files\bootstrap.bat" />
		<file src="build.bat" target="Files\build.bat" />
		<file src="deploy.ps1" target="Files\deploy.ps1" />
		<file src="main.build" target="Files\main.build" />
		<file src="servers.txt" target="Files\servers.txt" />
		<file src="settings.txt" target="Files\settings.txt" />
	</files>
</package>